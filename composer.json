{
    "name": "codefog/contao-faq-tags",
    "description": "FAQ Tags bundle for Contao Open Source CMS",
    "keywords": ["contao", "tag", "tags", "tagging", "faq"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "contao/faq-bundle": "^5.3",
        "codefog/tags-bundle": "^3.2"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls": "^2.4",
        "contao/test-case": "^4.2"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\FaqTagsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codefog\\FaqTagsBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\FaqTagsBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
